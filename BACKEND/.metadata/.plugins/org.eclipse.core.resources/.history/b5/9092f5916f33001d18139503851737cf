spring:
  application:
    name: gate-way
  
  cloud:
    gateway:
      discovery:
        locator:
          enabled: true
          
          lower-case-service-id: true
          
      routes:
      - id: customer-service
        uri:
          lb://customer-service
        predicates:
        - Path= /customer/** 
        
        
      - id: company-service
        uri:
          lb://company-service
        predicates:
        - Path= /mapcontroller/**